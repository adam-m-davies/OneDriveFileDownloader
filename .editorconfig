root = true

[*]
indent_style = tab
indent_size = tab
insert_final_newline = true
end_of_line = crlf
charset = utf-8
trim_trailing_whitespace = true

[*.{cs,csproj,props,targets}]
indent_style = tab
indent_size = tab

[*.{xaml,xml,json,md,txt}]
indent_style = tab
indent_size = tab
trim_trailing_whitespace = false
